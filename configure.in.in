  [CFLAGS="$CFLAGS -g -DDEBUG -DPKGDEP_DEBUG";
     CXXFLAGS="$CXXFLAGS -g -DDEBUG -DPKGDEP_DEBUG"],
  [CFLAGS="$CFLAGS -DNDEBUG";
     CXXFLAGS="$CXXFLAGS -DNDEBUG"])

## some common checks
@YAST2-CHECKS-COMMON@
@YAST2-CHECKS-LIB@

